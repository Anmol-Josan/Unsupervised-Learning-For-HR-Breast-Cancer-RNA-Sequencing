   "cell_type": "code",
   "execution_count": 30,
   "id": "de089fca",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2026-02-16T23:03:49.423755Z",
     "iopub.status.busy": "2026-02-16T23:03:49.423488Z",
