  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "22b1d959",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-11-12T03:57:36.474836Z",
     "iopub.status.busy": "2025-11-12T03:57:36.474509Z",
     "iopub.status.idle": "2025-11-12T03:59:19.121304Z",
     "shell.execute_reply": "2025-11-12T03:59:19.120217Z"
